<h2 class="page-header">Register</h2>
{{#if errors}}
      {{#each errors}}
        <div class="alert alert-danger">{{msg}}</div>
      {{/each}}
{{/if}}
<form method="post" action="/users/register">
   <div class="form-group">
    <label>Name</label>
    <input type="text" class="form-control" placeholder="Name" name="name">
  </div>
  <div class="form-group">
    <label>Last Name</label>
    <input type="text" class="form-control" placeholder="Username" name="username">
  </div>
  <div class="form-group">
    <label>Birthdate</label>
    <input type="date" class="form-control" placeholder="Nickeman" name="password2">
  </div>
   <div class="form-group">
    <label>Email</label>
    <input type="email" class="form-control" placeholder="Email" name="email">
  </div>
  <div class="form-group">
    <label>Password</label>
    <input type="password" class="form-control" placeholder="Password" name="password">
  </div>
  <div class="form-group">
    <label>Confirm Password</label>
    <input type="password" class="form-control" placeholder="Password" name="password2">
  </div>
  <div class="form-group">
    <label>Nickname</label>
    <input type="text" class="form-control" placeholder="Nickeman" name="nickname">
  </div>
  <div class="form-group">
    <label>Cell Phone Number</label>
    <input type="number" class="form-control" placeholder="CellPhoneNumber" name="CellPhoneNumber">
  </div>
  <div>
    <input type="radio" name="gender" value="male" checked> Male<br>
    <input type="radio" name="gender" value="female"> Female<br>
    <input type="radio" name="gender" value="other"> Other
  </div>
  <div class="form-group">
    <label>Nationnality</label>
    <input type="text" class="form-control" placeholder="Nationnality" name="nationality">
  </div>
  <div class="form-group">
    <label>Identification</label>
    <input type="text" class="form-control" placeholder="Identification" name="identification">
  </div>
  <div class="form-group">
    <label>Education Level</label>
    <input type="text" class="form-control" placeholder="education_level" name="educationlevel">
  </div>
  <div class="form-group">
    <label>Coding Expirence</label>
    <input type="text" class="form-control" placeholder="coding_experience" name="codingexperience">
  </div>
  <div class="form-group">
    <label>Personal Reference</label>
    <input type="text" class="form-control" placeholder="personal_information" name="personalInformation">
  </div>
  <div class="form-group">
    <label>Holacode Discovery</label>
    <input type="text" class="form-control" placeholder="holcaode_discovery" name="holacodeDiscovery">
  </div>
  <div class="form-group">
    <label>Commitment</label>
    <input type="text" class="form-control" placeholder="Commentmen" name="commintment">
  </div>
  <button type="submit" class="btn btn-default">Submit</button>
</form>
